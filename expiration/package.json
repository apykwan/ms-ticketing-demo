{
  "name": "expiration",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --watchAll --no-cache --runInBand"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "testMatch": [
      "**/__test__/**/*.test.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@apkmstickets/common": "^1.0.26",
    "bull": "^4.16.5",
    "node-nats-streaming": "^0.3.2"
  },
  "devDependencies": {
    "@types/bull": "^3.15.9",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  }
}
